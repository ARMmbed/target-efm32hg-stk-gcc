{
  "name": "efm32hg-stk-gcc",
  "version": "0.4.0",
  "description": "Official mbed build target for the SLSTK3400A Starter Kit for EFM32HG, using the GCC toolchain.",
  "license": "Apache-2.0",
  "inherits": {
    "efm32hg-gcc": "^1.0.0"
  },
  "keywords": [
    "mbed-target:efm32hg-stk",
    "mbed-target:efm32hg_stk3400",
    "mbed-official",
    "efm32hg-stk"
  ],
  "similarTo": [
    "efm32hg-stk"
  ],
  "config": {
    "hardware": {
      "device": "EFM32HG322F64",
      "flash-size": 64,
      "ram-size": 8,
      "clock": {
        "core": {
          "source": "HFXO",
          "frequency": 24000000
        },
        "low-energy": {
          "source": "LFXO",
          "frequency": 32768
        }
      },
      "pins": {
        "LED0": "PF4",
        "LED1": "PF5",
        "LED2": "LED0",
        "LED3": "LED0",
        "LED4": "LED1",
        "SW0": "PC9",
        "SW1": "PC10",
        "BTN0": "SW0",
        "BTN1": "SW1",
        "UART_TX": "PD4",
        "UART_RX": "PD5",
        "SERIAL_TX": "PD4",
        "SERIAL_RX": "PD5",
        "STDIO_UART_TX": "PF2",
        "STDIO_UART_RX": "PA0",
        "USBTX": "PF2",
        "USBRX": "PA0",
        "EFM_BC_EN": "PA9"
      },
      "test-pins": {
        "spi": {
          "MOSI": "PE10",
          "MISO": "PE11",
          "SCLK": "PE12",
          "SSEL": "PE13"
        },
        "i2c": {
          "SDA": "PC0",
          "SCL": "PC1"
        }
      }
    },
    "modules": {
      "serial": {
        "stdio-uart": "USART1"
      }
    }
  },
  "scripts": {
    "debug": [
      "valinor",
      "-t",
      "uvision",
      "--target",
      "efm32hg-stk",
      "$program"
    ],
    "test": [
      "mbed_test_wrapper",
      "--target",
      "EFM32HG_STK3400",
      "$program"
    ]
  }
}
