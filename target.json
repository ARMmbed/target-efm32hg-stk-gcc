{
  "name": "efm32hg-stk-gcc",
  "version": "0.0.1",
  "inherits": {
    "mbed-gcc": "*"
  },
  "description": "GCC target description for the SLSTK3400A Starter Kit for EFM32HG.",
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    }
  ],
  "keywords": [
    "mbed-target:efm32hg",
    "gcc"
  ],
  "similarTo": [
    "silabs",
    "efm32",
    "efm32hg",
    "efm32hg-stk",
    "cortex-m0plus",
    "armv6-m",
    "arm",
    "gcc"
  ],
  "config": {
    "minar": {
      "initial_event_pool_size": 10,
      "additional_event_pools_size": 10
    }
    },
  "toolchain": "CMake/toolchain.cmake",
  "debug-server": [
    "JLinkGDBServer",
    "-if",
    "SWD",
    "-speed",
    "4000",
    "-device",
    "EFM32HG322F64",
    "-log",
    "/tmp/jlink-efm.log"
  ],
  "scripts": {
    "debug": [
      "valinor",
      "-t",
      "uvision",
      "--target",
      "efm32hg-stk",
      "$program"
    ]
  }
}